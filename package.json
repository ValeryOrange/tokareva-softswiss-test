    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.16.0",
    "sass": "^1.85.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "overrides": {
    "react-refresh": "^0.16.0"
